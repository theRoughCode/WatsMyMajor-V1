<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>WatsMyMajor</title>
  <link rel="icon" href="/public/images/favicon.ico">
  <link rel="stylesheet" type="text/css" href="/stylesheets/style.css">
</head>
<body>
  <nav>
    <a href="/">Home</a>
    {{#if prereqs}}
      <a href="/trees/{{ subject }}/{{ catalog_number }}">View Prerequisites Tree</a>
    {{/if}}
    {{#if url}}
      <a href="{{ url }}">Course Information</a>
    {{/if}}
  </nav>
  <h1>{{ course }}</h1>
  <p>Offered in: {{ terms }}</p>
  <div class="words" contenteditable>
    {{ description }}
  </div>
  {{#if prereqs}}
    <h2>Prerequisites:</h2>
    {{#each string.prereqs}}
      <li>{{ this }}</li>
    {{/each}}
  {{/if}}
  {{#if coreqs}}
    <h2>Corequisites</h2>
    {{#each string.coreqs}}
      <li>{{ this }}</li>
    {{/each}}
  {{/if}}
  {{#if antireqs}}
    <h2>Antirequisites</h2>
    {{#each antireqs}}
      <li>{{ this }}</li>
    {{/each}}
  {{/if}}
  {{#if crosslistings}}
    <h2>Cross Listings</h2>
    <li>{{ crosslistings }}</li>
  {{/if}}
  {{#if parPrereq}}
    <h2>Requires {{ subject }} {{ catalog_number }} As A Prerequisite</h2>
    {{#each parPrereq}}
      <li>{{ this.subject }} {{ this.cat_num}}</li>
    {{/each}}
  {{/if}}
  {{#if parCoreq}}
    <h2>Requires {{ subject }} {{ catalog_number }} As A Corequisite</h2>
    {{#each parCoreq}}
      <li>{{ this.subject }} {{ this.cat_num}}</li>
    {{/each}}
  {{/if}}
</body>
</html>
